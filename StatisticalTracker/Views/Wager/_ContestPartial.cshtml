@model IEnumerable<StatisticalTracker.Models.ContestModel>
@{
    ViewBag.Title = "Contests";
}

<table>
    <tbody>
        
            @foreach (var m in Model)
            {
                <tr>
                    <td>@m.EntryId</td>
                    <td>@m.Sport</td>
                    <td>@m.Date</td>
                    <td>@m.ContestTitle</td>
                    <td>@m.SalaryCap</td>
                    <td>@m.Score</td>
                    <td>@m.Position</td>
                    <td>@m.Entries</td>
                    <td>@m.Opponent</td>
                    <td>@m.EntryFee</td>
                    <td>@m.Winnings</td>
                    <td>@m.Link</td>

                </tr>
            }
        

    </tbody>
</table>


